
(rules PCB ltl
  (snap_angle 
    fortyfive_degree
  )
  (autoroute_settings
    (fanout off)
    (autoroute on)
    (postroute on)
    (vias on)
    (via_costs 50)
    (plane_via_costs 5)
    (start_ripup_costs 100)
    (start_pass_no 100000)
    (layer_rule F.Cu
      (active on)
      (preferred_direction horizontal)
      (preferred_direction_trace_costs 1.0)
      (against_preferred_direction_trace_costs 2.6)
    )
    (layer_rule B.Cu
      (active on)
      (preferred_direction vertical)
      (preferred_direction_trace_costs 1.0)
      (against_preferred_direction_trace_costs 1.6)
    )
  )
  (rule
    (width 250.0)
    (clear 200.2)
    (clear 125.0 (type smd_to_turn_gap))
  )
  (padstack "Via[0-1]_800:400_um"
    (shape
      (circle F.Cu 800.0 0.0 0.0)
    )
    (shape
      (circle B.Cu 800.0 0.0 0.0)
    )
    (attach off)
  )
  (via 
    "Via[0-1]_800:400_um" "Via[0-1]_800:400_um" default
  )
  (via 
    "Via[0-1]_800:400_um-kicad_default" "Via[0-1]_800:400_um" "kicad_default"
  )
  (via_rule
    default "Via[0-1]_800:400_um"
  )
  (via_rule
    "kicad_default" "Via[0-1]_800:400_um-kicad_default"
  )
  (class default
    (clearance_class default)
    (via_rule default)
    (rule
      (width 250.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
  (class "kicad_default"
    GND +3V3 "unconnected-(J1-Pin_1-Pad1)" "unconnected-(J1-Pin_3-Pad3)" +4V "Net-(U1-DONE)" DTR RESET
    "Net-(Flash1-Pad1)" "Net-(Q1-D)" "Net-(Q2-D)" "Net-(CSO1-Pin_1)" /TxD "Net-(U2-CH_PD)" "Net-(U2-TXD)" "Net-(U2-RXD)"
    DONE "Net-(U1-~{DRV})" DRIVE "BAT_DIV" "Net-(GPIO9-Pin_1)" "Net-(GPIO10-Pin_1)" "Net-(GPIO12-Pin_1)" "Net-(GPIO13-Pin_1)"
    "Net-(GPIO14-Pin_1)" "Net-(GPIO16-Pin_1)" "Net-(MISO1-Pin_1)" "Net-(MOSI1-Pin_1)" FLASH "Net-(U2-GPIO2)" "Net-(SCL1-Pin_1)" "unconnected-(U4-NR-Pad4)"
    /RxD "Net-(SCLK1-Pin_1)" "Net-(SDA1-Pin_1)" DELAY "Net-(Q3-C)" "Net-(Q3-B)" "Net-(D1-A)"
    (clearance_class "kicad_default")
    (via_rule "kicad_default")
    (rule
      (width 250.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
)